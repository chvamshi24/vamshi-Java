package Getters;

class Person {
	String name;
	int age;
	
	void speak() {
		System.out.println("My name is: " + name);
	}
}

public class getters {

	public static void main(String[] args) {
		Person person1 = new Person();
		
		person1.name = "Vamshi";
		person1.age = 25;
		
		person1.speak();
	}

}
